<table>
  <tr>
    <td>
      ID
    </td>
    <td>
      Name
    </td>
    <td>
      Able to serve alcohol?
    </td>
  </tr>
  <% Employee.all.each do |x| %>
    <tr>
      <td>
        <%= x.id %>
      </td>
      <td>
        <%= x.name %>
      </td>
      <td>
        <% if x.can_serve_booze? %>
          Yes
        <% else %>
          No
        <% end %>
      </td>
    </tr>
  <% end %>
</table>
